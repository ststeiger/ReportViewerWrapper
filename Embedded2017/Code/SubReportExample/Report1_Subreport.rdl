  <DataSets>
    <DataSet Name="GroupsDetails">
      <Query>
        <DataSourceName>COR_Basic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MyUser">
            <Value>=Parameters!MyUser.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT ID AS BG_ID, Name AS BG_Name 
FROM T_Benutzergruppen
LEFT JOIN T_Benutzer_Benutzergruppen ON T_Benutzer_Benutzergruppen.BEBG_BG = T_Benutzergruppen.ID 
WHERE (1=1) 
AND T_Benutzer_Benutzergruppen.BEBG_BE = @MyUser </CommandText>
        <Timeout>300</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
